{
  "scripts": {
    "start": "npm-run-all --parallel css eleventy",
    "dev": "yarn start",
    "eleventy": "cross-env ELEVENTY_EXPERIMENTAL=true eleventy --serve",
    "debug": "set DEBUG=* & eleventy",
    "css": "postcss src/static/css/tailwind.css --o _tmp/static/css/style.css",
    "build": "cross-env ELEVENTY_EXPERIMENTAL=true NODE_ENV=production eleventy && cross-env NODE_ENV=production postcss src/static/css/tailwind.css --o _site/static/css/style.css"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.1.3",
    "@asciidoctor/core": "^2.2.5",
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@tailwindcss/typography": "^0.5.0",
    "alpinejs": "^3.7.0",
    "asciidoctor": "^2.2.5",
    "asciidoctor-html5s": "^0.5.1",
    "asciidoctor-prism-extension": "^2.0.6",
    "autoprefixer": "^10.4.0",
    "cross-env": "^7.0.3",
    "cssnano": "^5.0.12",
    "eleventy-plugin-nesting-toc": "^1.3.0",
    "eleventy-plugin-srcset": "^0.4.0",
    "eta": "^1.12.3",
    "html-minifier": "^4.0.0",
    "js-yaml": "^4.1.0",
    "jsdom": "^19.0.0",
    "lodash": "^4.17.21",
    "luxon": "^2.1.1",
    "markdown-it": "^12.3.0",
    "markdown-it-anchor": "^8.4.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.4",
    "postcss-cli": "^9.0.2",
    "postcss-import": "^14.0.2",
    "postcss-nested": "^5.0.6",
    "prismjs": "^1.25.0",
    "tailwindcss": "^3.0.0",
    "yarn-upgrade-all": "^0.5.4"
  }
}
