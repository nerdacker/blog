---
layout: redirect
pagination:
  data: site.languages
  size: 1
  alias: language
  addAllPagesToCollections: true
eleventyComputed:
  title: "Redirect"
  metaTitle: "{{ site.meta[language.locale].title }} - {{ title }}"
  locale: "{{ language.locale }}"
  localeRegion: "{{ language.localeRegion }}"
  language: "{{ language.localeRegion }}"
destination: "blog/index.html"
permalink: /{{language.locale}}/index.html
